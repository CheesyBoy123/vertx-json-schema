{
  "openapi" : "3.0.1",
  "info" : {
    "license" : {
      "name" : "Apache 2.0",
      "url" : "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "contact" : {
      "email" : "apiteam@swagger.io"
    },
    "description" : "This is a sample server Petstore server.  You can find out more about     Swagger\nat [http://swagger.io](http://swagger.io) or on [irc.freenode.net, #swagger](http://swagger.io/irc/).      For\nthis sample, you can use the api key `special-key` to test the authorization     filters.",
    "termsOfService" : "http://swagger.io/terms/",
    "title" : "Swagger Petstore",
    "version" : "1.0.0"
  },
  "servers" : [ {
    "url" : "https://petstore.swagger.io/v2"
  }, {
    "url" : "http://petstore.swagger.io/v2"
  } ],
  "externalDocs" : {
    "description" : "Find out more about Swagger",
    "url" : "http://swagger.io"
  },
  "tags" : [ {
    "name" : "pet",
    "description" : "Everything about your Pets",
    "externalDocs" : {
      "description" : "Find out more",
      "url" : "http://swagger.io"
    }
  }, {
    "name" : "store",
    "description" : "Access to Petstore orders"
  }, {
    "name" : "user",
    "description" : "Operations about user",
    "externalDocs" : {
      "description" : "Find out more about our store",
      "url" : "http://swagger.io"
    }
  } ],
  "components" : {
    "schemas" : {
      "Order" : {
        "$ref" : "https://vertx.io/store.model.json#/Order"
      },
      "User" : {
        "$ref" : "https://vertx.io/user.model.json#/User"
      },
      "Category" : {
        "$ref" : "https://vertx.io/pet.model.json#/Category"
      },
      "Tag" : {
        "$ref" : "https://vertx.io/pet.model.json#/Tag"
      },
      "Pet" : {
        "$ref" : "https://vertx.io/pet.model.json#/Pet"
      },
      "ApiResponse" : {
        "$ref" : "https://vertx.io/pet.model.json#/ApiResponse"
      }
    },
    "securitySchemes" : {
      "petstore_auth" : {
        "flows" : {
          "implicit" : {
            "authorizationUrl" : "http://petstore.swagger.io/oauth/dialog",
            "scopes" : {
              "write:pets" : "modify pets in your account",
              "read:pets" : "read your pets"
            }
          }
        },
        "type" : "oauth2"
      },
      "api_key" : {
        "in" : "header",
        "name" : "api_key",
        "type" : "apiKey"
      }
    }
  },
  "paths" : {
    "/pet" : {
      "$ref" : "https://vertx.io/pet.api.json#/pet"
    },
    "/user/{username}" : {
      "$ref" : "https://vertx.io/user.api.json#/user-username"
    },
    "/pet/findByStatus" : {
      "$ref" : "https://vertx.io/pet.api.json#/pet-findByStatus"
    },
    "/user/createWithList" : {
      "$ref" : "https://vertx.io/user.api.json#/user-createWithList"
    },
    "/pet/{petId}/uploadImage" : {
      "$ref" : "https://vertx.io/pet.api.json#/pet-petId-uploadImage"
    },
    "/store/inventory" : {
      "$ref" : "https://vertx.io/store.api.json#/store-inventory"
    },
    "/user/login" : {
      "$ref" : "https://vertx.io/user.api.json#/user-login"
    },
    "/user" : {
      "$ref" : "https://vertx.io/user.api.json#/user"
    },
    "/user/createWithArray" : {
      "$ref" : "https://vertx.io/user.api.json#/user-createWithArray"
    },
    "/pet/findByTags" : {
      "$ref" : "https://vertx.io/pet.api.json#/pet-findByTags"
    },
    "/store/order" : {
      "$ref" : "https://vertx.io/store.api.json#/store-order"
    },
    "/user/logout" : {
      "$ref" : "https://vertx.io/user.api.json#/user-logout"
    },
    "/pet/{petId}" : {
      "$ref" : "https://vertx.io/pet.api.json#/pet-petId"
    },
    "/store/order/{orderId}" : {
      "$ref" : "https://vertx.io/store.api.json#/store-order-orderId"
    }
  }
}
